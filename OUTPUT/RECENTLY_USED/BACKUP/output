- @PreAuthorize("checkPermission(#id, 'SavedHQL', 'HQLReportDelete')")
- @PreAuthorize("checkPermission('HQLReports')")
+ @PreAuthorize("checkPermission(#request.id, 'SavedHQL', 'HQLReportDelete')")
